<div>
<table>
<tr>
<th>Name</th>
<th>Start date</th>
<th>End date</th>
<th>Teacher</th>
<th>Course</th>
</tr>
<% @cohorts.each do |cohort|%>
    <tr>
    <th><%=cohort.name%></th>
    <th><%=cohort.start_date%></th>
    <th><%=cohort.end_date%></th>
   <%current_teacher = @teachers.find(cohort.teacher_id)%>
   <th><%=current_teacher.first_name%> <%=current_teacher.last_name%></p></th>
   <%current_course = @courses.find(cohort.course_id)%>
   <th><%=current_course.name%></th>
   </tr>
   <% end %>
</table>

</div> 