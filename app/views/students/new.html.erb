<%= form_with model: @student do |form| %>
  <%= form.text_field :first_name %>
  <%= form.text_field :last_name %>
  <%= form.text_field :age %>
  <%= form.text_field :cohort_id %>
  <%= form.text_field :email %>
  <%= form.text_field :phone_number %>
  <%= form.text_field :img_url %>
  <%= form.submit %>
<% end %>
